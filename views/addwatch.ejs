<%- include header %>
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header"> 添加监控项目
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            <div class="panel panel-primary">
                <div class="panel-body">
                    <form class="form-horizontal" role="form" method="post">
                        <div class="row">
                            <div class="form-group">
                                <label class="col-lg-3 control-label">车牌号</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="licence" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">司机工号</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="id" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">司机姓名</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="name" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">司机性别</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="sex" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">司机年龄</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="age" type="text">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-lg-3 control-label">司机电话</label>
                                <div class="col-lg-7 ">
                                    <input class="form-control" name="phone" type="text">
                                </div>
                            </div>
                            <div  class="col-lg-offset-8">
                                <button type="submit" class="btn btn-default">保存</button>
                            </div>
                        </div>
                    </form>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->
<%- include footer %>